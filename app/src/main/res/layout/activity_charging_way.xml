<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGrey"
    android:orientation="vertical"
    tools:context="com.tangchaoke.electrombilechargingpile.activity.ChargingWayActivity">


    <com.tangchaoke.electrombilechargingpile.view.NiceRecyclerView
        android:id="@+id/wa_power"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:listDirection="VERTICAL" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="预约充电（敬请期待）"
        android:textColor="@color/hintColor"
        android:textSize="15sp" />
</LinearLayout>

